    "react-select": "^2.1.2",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-saga": "^0.16.2",
    "redux-saga-test-plan": "^3.7.0",
    "redux-thunk": "^2.3.0",
    "resolve": "1.8.1",
